{
  "name": "ChromeTM",
  "version": "1.0.1",
  "description": "Ssystem monitor and task manager for chrome",
  "permissions": [
    "storage",
    "system.cpu",
    "system.memory",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/system16.png",
      "32": "images/system32.png",
      "48": "images/system128.png",
      "128": "images/system128.png"
    }
  },
  "icons": {
    "16": "images/system16.png",
    "32": "images/system32.png",
    "48": "images/system128.png",
    "128": "images/system128.png"
  },
  "manifest_version": 2
}